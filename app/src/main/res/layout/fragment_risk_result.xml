<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wehealth.pdqbook.fragment.RiskResultFragment">


    <com.wehealth.pdqbook.view.ColorArcProgressBar
        android:id="@+id/score_progress_bar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        app:is_need_content="true"
        app:max_value="100"
        app:back_width="20dp"
        app:front_width="19dp"
        app:total_engle="270"
        app:is_need_unit="true"
        app:string_unit="km/h"
        app:is_need_title="false"
        app:string_title=""
        app:back_color="@android:color/darker_gray" />

</RelativeLayout>
